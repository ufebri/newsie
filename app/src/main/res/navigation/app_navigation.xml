<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.raytalktech.newsie.ui.home.HomeFragment"
        android:label="@string/bottomBarNavigation_Home"
        tools:layout="@layout/content_home_fragment" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.raytalktech.newsie.ui.search.SearchFragment"
        android:label="@string/search"
        tools:layout="@layout/content_search_fragment" />

    <fragment
        android:id="@+id/navigation_bookmark"
        android:name="com.raytalktech.newsie.ui.bookmark.BookMarkFragment"
        android:label="@string/bottomBarNavigation_Home"
        tools:layout="@layout/content_bookmark_fragment" />

